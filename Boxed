public class BoxingExample {
    public static void main(String[] args) {
        Integer i = 42;
        Long l = Long.valueOf(i.longValue()); // ðŸš¨ unnecessary unbox + rebox
        Boolean b = Boolean.valueOf(Boolean.valueOf("true")); // ðŸš¨ double box
        System.out.println(l + " " + b);
    }
}
